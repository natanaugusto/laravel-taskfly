                    "404": {
                        "description": "Not Found"
                    },
                    "422": {
                        "description": "Validation error"
                    }
                }
            },
            "delete": {
                "operationId": "3cf47151dcf092ecf26e634b08a6188a",
                "responses": {
                    "202": {
                        "description": "Deleted a task"
                    },
                    "404": {
                        "description": "Not Found"
                    }
                }
            }
        }
    }
